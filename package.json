{
  "name": "scrum-presentation",
  "version": "0.1.0",
  "description": "Scrum overview based in SBOK",
  "author": "Ronald Lima",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kofre/scrum-presentation"
  },
  "homepage": "https://kofre.github.io/scrum-presentation",
  "license": "ISC",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "push-dir --verbose --dir=dist --branch=gh-pages",
    "release": "yarn build && yarn deploy"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "chart.js": "2.5.0",
    "eagle.js": "^0.4.1",
    "highlight.js": "^9.12.0",
    "vue": "^2.1.10",
    "vue-router": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^3.0.4",
    "node-sass": "^4.9.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
